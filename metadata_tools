[
    {
        "column_name": "CAT_NR",
        "alias": "CATALOGUE NUMBER\"; unique number assigned to the archaeological object in the final Catalogue of Finds"
    },
    {
        "column_name": "FIND_ID",
        "alias": "FIND ID NUMBER\"; unique number generated by the database, assigned to each spindle whorl drawing upon recording"
    },
    {
        "column_name": "INV_NR",
        "alias": "INVENTORY NUMBER\"; identifier number assigned to the archaeological object upon its storage in the holding institution"
    },
    {
        "column_name": "EXC_NR",
        "alias": "EXCAVATION NUMBER\"; identifier number assigned to the archaeological object upon its recovery during the excavation"
    },
    {
        "column_name": "EXC_TAG",
        "alias": "EXCAVATION TAG\";  vital information containing details about the archaeological object's context (at excavation entrance)"
    },
    {
        "column_name": "NOTES",
        "alias": "ADDITIONAL INFORMATION NOTES\"; additional remarks about the archaeological object given as an observation"
    },
    {
        "column_name": "INSTITUTION",
        "alias": "HOLDING INSTITUTION\"; full name of an institution in which the archaeological object was stored upon recording"
    },
    {
        "column_name": "SITE",
        "alias": "ARCHAEOLOGICAL SITE\"; full name of an archaeological site on which the archaeological object was recovered"
    },
    {
        "column_name": "CONTEXT",
        "alias": "CULTURE-HISTORICAL CONTEXT\"; culture-historical context to which the archaeological object belongs to"
    },
    {
        "column_name": "T_SLICE",
        "alias": "TIME SLICE\"; approximate time frame in which the archaeological object can be dated by quarter millennia"
    },
    {
        "column_name": "TYPE",
        "alias": "TYPE\"; typological designation based on the overall form of an archaeological object (separate categories for spindle whorls () and loom weights ())"
    },
    {
        "column_name": "PRESERVATION",
        "alias": "PRESERVATION CONDITION\"; state in which the object was documented and recorded (\"complete\", \"half\", \"partial\" and \"small fragments missing\")"
    },
    {
        "column_name": "DECO",
        "alias": "DECORATION\"; presence of any kind of decorative design on the archaeological object"
    },
    {
        "column_name": "W_C",
        "alias": "WEIGHT IF COMPLETE\"; \"complete\" archaeological object's measured weight in grams"
    },
    {
        "column_name": "W_I",
        "alias": "WEIGHT IF NOT COMPLETE\"; incomplete archaeological object's measured weight in grams ( \"partial\" objects, objects preserved in \"half\", objects that have \"small fragments missing\")"
    },
    {
        "column_name": "W_C_E",
        "alias": "ESTIMATED COMPLETE WEIGHT\"; estimation of complete weight in grams for archaeological objects (spindle whorls) that have \"small fragments missing\" (equals \"weight if not complete\")"
    },
    {
        "column_name": "W_CAL",
        "alias": "CALCULATED COMPLETE WEIGHT\"; calculation of complete weight in grams for archaeological objects (spindle whorls) that were preserved in \"half\" (equals \"weight if  not complete\" doubled)"
    },
    {
        "column_name": "W_REC",
        "alias": "RECONSTRUCTED COMPLETE WEIGHT\"; reconstruction of complete weight  for \"partial\" archaeological objects (spindle whorls) in grams (equals \"density\" times \"complete volume\")"
    },
    {
        "column_name": "V_C",
        "alias": "COMPLETE VOLUME\"; volume of the virtual three dimensional reconstruction of the archaeological object (spindle whorl)"
    },
    {
        "column_name": "V_F",
        "alias": "VOLUME OF THE FRAGMENT\"; volume of the \"partial\" archaeological object (spindle whorl's fragment) generated from its virtual three dimensional model"
    },
    {
        "column_name": "DEN",
        "alias": "DENSITY\"; archaeological object's (spindle whorl's) density calculated from its \"volume of a fragment\" and its \"weight if not complete\" (equals \"weight if not complete\" divided by \"volume of the fragment\" )"
    },
    {
        "column_name": "H",
        "alias": "HEIGHT\"; complete height of the archaeological object (spindle whorl or loom weight) measured in millimeters"
    },
    {
        "column_name": "D",
        "alias": "DIAMETER\"; complete diameter of the archaeological object (spindle whorl) measured in millimeters"
    },
    {
        "column_name": "P",
        "alias": "PERFORATION DIAMETER\"; diameter of the archaeological object's (spindle whorl's or loom weight's) perforation measured with a caliper on the inside of a tunnel in millimeters"
    },
    {
        "column_name": "T",
        "alias": "THICKNESS\"; complete thickness of the archaeological object (loom weight) measured in millimeters"
    },
    {
        "column_name": "W",
        "alias": "WIDTH\"; complete width of the archaeological object (loom weight) measured in millimeters"
    },
    {
        "column_name": "L",
        "alias": "LENGTH\"; complete length of the archaeological object (spool) measured in millimeters"
    },
    {
        "column_name": "D_MAX",
        "alias": "MAXIMUM DIAMETER\"; complete maximum diameter of the archaeological object (spool) measured in millimeters"
    },
    {
        "column_name": "D_MIN",
        "alias": "MINIMUM DIAMETER\"; complete minimum diameter of the archaeological object (spool) measured in millimeters"
    },
    {
        "column_name": "MATERIAL",
        "alias": "OBJECT MATERIAL\"; material out of which the archaeological object has been made from"
    },
    {
        "column_name": "LAT",
        "alias": "ARCHAEOLOGICAL SITE LATITUDE\"; the archaeological site's geographical latitude coordinate"
    },
    {
        "column_name": "LON",
        "alias": "ARCHAEOLOGICAL SITE LONGITUDE\"; the archaeological site's geographical longitude coordinate"
    }
